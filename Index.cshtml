@model List<User>

<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Ім'я</th>
            <th>Прізвище</th>
            <th>Пошта</th>
            <th>Дата реєстрації</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Id</td>
                <td>@user.FirstName</td>
                <td>@user.LastName</td>
                <td>@user.Email</td>
                <td>@user.RegistrationDate.ToString("dd.MM.yyyy")</td>
            </tr>
        }
    </tbody>
</table>
